cc_binary {
    name: "folio-daemon",

    init_rc: ["folio_daemon.rc"],

    shared_libs: [
        "libandroid",
        "libcutils",
        "liblog",
    ],

    srcs: ["main.cpp"],

    cflags: [
        "-DLOG_TAG=\"folio-daemon\"",
        "-DLOG_NDEBUG=0",
    ] + [
        "-Wall",
        "-Werror",
    ],

    owner: "google",

    system_ext_specific: true,
}
